			<iSignal Name="CMDfb_R" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10030" />
			<iSignal Name="CMDfb_R2" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10030" />
			
			<oSignal Name="CMD_L" To="baxtorRobotPlantCD.CMD_L" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10031" />
