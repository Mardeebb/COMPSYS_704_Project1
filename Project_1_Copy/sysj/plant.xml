			<iSignal Name="gripperTurnExtend"    Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10041" />
			<iSignal Name="capGripperPos5Extend"      Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10041" />
			<iSignal Name="cylClampBottleExtend"       Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10041" />
			<iSignal Name="enable"       Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10041" />
		
			<oSignal Name="bottleAtPos4"  To="CapperControllerCD.bottleAtPos4" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10042" />
			<oSignal Name="gripperZAxisLowered"  To="CapperControllerCD.gripperZAxisLowered" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10042" />
			<oSignal Name="gripperZAxisLifted"  To="CapperControllerCD.gripperZAxisLifted" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10042" />
			<oSignal Name="gripperTurnHomePos"  To="CapperControllerCD.gripperTurnHomePos" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10042" />
			<oSignal Name="gripperTurnFinalPos"  To="CapperControllerCD.gripperTurnFinalPos" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10042" />
			
			<oSignal Name="gripperZAxisLoweredE"           To="UI.gripperZAxisLoweredE"           Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="gripperZAxisLiftedE"           To="UI.gripperZAxisLiftedE"           Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />	
			<oSignal Name="gripperTurnHomePosE"           To="UI.gripperTurnHomePosE"           Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="gripperTurnFinalPosE"           To="UI.gripperTurnFinalPosE"           Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />	
		</ClockDomain>

		
		<ClockDomain Name="rotaryTablePlantCD" Class="rotPlant">
			<iSignal Name="rotaryTableTrigger" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10002" />
			<iSignal Name="enable" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10002" />

			<oSignal Name="tableAlignedWithSensorC" To="rotaryTableControllerCD.tableAlignedWithSensorC" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10003" />
			
