<System xmlns="http://systemjtechnology.com">
	<SubSystem Name="SS2" Local="true">
		<ClockDomain Name="CapLoaderPlantCD" Class="CapLoaderPlant">
			<iSignal Name="pusherExtend" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="vacOn"        Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="armSource"    Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="armDest"      Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="enable"       Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="refill"       Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />

			<oSignal Name="pusherRetracted" To="CapLoaderControllerCD.pusherRetracted" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />
			<oSignal Name="pusherExtended"  To="CapLoaderControllerCD.pusherExtended"  Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />
			<oSignal Name="WPgripped"       To="CapLoaderControllerCD.WPgripped"       Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />
			<oSignal Name="armAtSource"     To="CapLoaderControllerCD.armAtSource"     Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />
			<oSignal Name="armAtDest"       To="CapLoaderControllerCD.armAtDest"       Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />
			<oSignal Name="empty"           To="CapLoaderControllerCD.empty"           Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />

			<oSignal Name="pusherRetractedE" To="UI.pusherRetractedE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="pusherExtendedE"  To="UI.pusherExtendedE"  Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="WPgrippedE"       To="UI.WPgrippedE"       Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="armAtSourceE"     To="UI.armAtSourceE"     Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="armAtDestE"       To="UI.armAtDestE"       Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="emptyE"           To="UI.emptyE"           Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
		</ClockDomain>
		
		<ClockDomain Name="CapperPlantCD" Class="CapperPlant">
			<iSignal Name="cylPos5ZaxisExtend" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10005" />
			<iSignal Name="gripperTurnRetracted"        Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10005" />
			<iSignal Name="gripperTurnExtend"    Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10005" />
			<iSignal Name="capGripperPos5Extend"      Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10005" />
			<iSignal Name="cylClampBottleExtend"       Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10005" />
			<iSignal Name="enable"       Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10005" />
		
			<oSignal Name="bottleAtPos4"  To="CapperControllerCD.bottleAtPos4" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10003" />
			<oSignal Name="gripperZAxisLowered"  To="CapperControllerCD.gripperZAxisLowered" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10003" />
			<oSignal Name="gripperZAxisLifted"  To="CapperControllerCD.gripperZAxisLifted" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10003" />
			<oSignal Name="gripperTurnHomePos"  To="CapperControllerCD.gripperTurnHomePos" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10003" />
			<oSignal Name="gripperTurnFinalPos"  To="CapperControllerCD.gripperTurnFinalPos" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10003" />
			
			<oSignal Name="gripperZAxisLoweredE"           To="UI.gripperZAxisLoweredE"           Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="gripperZAxisLiftedE"           To="UI.gripperZAxisLiftedE"           Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />	
			<oSignal Name="gripperTurnHomePosE"           To="UI.gripperTurnHomePosE"           Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="gripperTurnFinalPosE"           To="UI.gripperTurnFinalPosE"           Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />	
		</ClockDomain>
		
		
		<ClockDomain Name="RotaryTablePlantCD" Class="RotaryTablePlant">
			<iSignal Name="rotaryTableTrigger" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10002" />
			
			<oSignal Name="tableAlignedWithSensorC" To="RotaryTableControllerCD.tableAlignedWithSensorC" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10003" />
			
			<oSignal Name="rotTurn" To="UI.rotTurn" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="rotContent" To="UI.rotContent" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="bottleAtPos5" To="UI.bottleAtPos5" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="tableAlignedWithSensor" To="UI.tableAlignedWithSensor" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="capOnBottleAtPos1" To="UI.capOnBottleAtPos1" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />

		</ClockDomain>
		
	</SubSystem>
</System>
